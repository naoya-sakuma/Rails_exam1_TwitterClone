<header>
<a>Twitter</a>
</header>
<h3>以下の内容で、送信する。</h3>
<%= form_with(model: @mussitation, local: true) do |form| %>
  <%= form.hidden_field :content %>
  <p>投稿内容:<%= @mussitation.content %></p>
  <%= form.submit  "登録" %>
  <%= form.submit '戻る', name: 'back' %>
<% end %>
<footer>
<p>株式会社twitter</p>
</footer>
